       <fo:table-cell background-color="antiquewhite" border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" font-weight="bold" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Flower</fo:block></fo:table-cell>
       <fo:table-cell background-color="antiquewhite" border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" font-weight="bold" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Type</fo:block></fo:table-cell>
       <fo:table-cell background-color="antiquewhite" border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" font-weight="bold" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Soil</fo:block></fo:table-cell>
      </fo:table-row>
     </fo:table-header>
     <fo:table-body>
      <fo:table-row>
       <fo:table-cell border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Chrysanthemum</fo:block></fo:table-cell>
       <fo:table-cell border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">perennial</fo:block></fo:table-cell>
       <fo:table-cell border-top-color="black" border-top-style="solid" border-top-width="1pt" text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">well drained</fo:block></fo:table-cell>
      </fo:table-row>
      <fo:table-row>
       <fo:table-cell text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Gardenia</fo:block></fo:table-cell>
       <fo:table-cell text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">perennial</fo:block></fo:table-cell>
       <fo:table-cell text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">acidic</fo:block></fo:table-cell>
      </fo:table-row>
      <fo:table-row>
       <fo:table-cell text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">Gerbera</fo:block></fo:table-cell>
       <fo:table-cell text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">annual</fo:block></fo:table-cell>
       <fo:table-cell text-align="from-table-column()"><fo:block end-indent="3pt" space-after="3pt" space-after.conditionality="retain" space-before="3pt" space-before.conditionality="retain" start-indent="3pt">sandy,well-drained</fo:block></fo:table-cell>
